{
  "compilerOptions": {
    "target": "ES2020",
    "newLine": "lf",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node"
  },
  "include": ["./src"]
}
